      tag: Runtime
    ScriptableComponent:
      script: coreLoop.lua
    TextComponent:
      position: [ 20.0, 20.0 ]
      scale: 1.0
      colour: [ 1.0, 1.0, 1.0 ]
      shader: font.shader
      font: arial.ttf
      text: ""

  - TagComponent:
      tag: Water
    TransformComponent:
      translation: [ 0.0, -117.5, 0.0 ]
      orientation: [ 0.0, 0.0, 0.0 ]
      scale: [ 10000.0, 0.0, 10000.0 ]
    WaterComponent:
      active: true
      shader: water.shader
      dudv: waterDUDV.png
      normal: NormalMap.png

  - TagComponent:
      tag: Terrain
    TransformComponent:
      translation: [ 0.0, 0.0, 0.0 ]
      orientation: [ 0.0, 0.0, 0.0 ]
      scale: [ 3000.0, 250.0, 3000.0 ]
    TerrainComponent:
      active: true
      terrain: terrain.jpg
      shader: terrain.shader
      textures:
        - texture: grass.jpg
          range: 0.1
        - texture: dirt.jpg
          range: 0.5
        - texture: rock.jpg
          range: 1.0
    RigidBodyComponent:
      mass: 1
      gravity: false
      type: static
    HeightMapColliderComponent:
      isTrigger: false

  - TagComponent:
      tag: Skybox
    SkyboxComponent:
        active: true
        shader: skybox.shader
