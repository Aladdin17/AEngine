entities:
  - TagComponent:
      tag: Skybox
    SkyboxComponent:
        active: true
        shader: skybox.shader
        texturePaths:
          - assets/textures/skybox/right.jpg
          - assets/textures/skybox/left.jpg
          - assets/textures/skybox/top.jpg
          - assets/textures/skybox/bottom.jpg
          - assets/textures/skybox/front.jpg
          - assets/textures/skybox/back.jpg
  - TagComponent:
      tag: Runtime
    ScriptableComponent:
      script: coreLoop.lua
  - TagComponent:
      tag: Floor
    TransformComponent:
      translation: [ -20.0, -20.0, -100.0 ]
      orientation: [ 0.0, 0.0, 0.0 ]
      scale: [ 1.0, 1.0, 1.0 ]
    RigidBodyComponent:
      mass: 0.0
      gravity: false
      type: kinematic
    BoxColliderComponent:
      trigger: false
      size: [ 500.0, 10.0, 500.0 ]

  - TagComponent:
      tag: Player
    TransformComponent:
      translation: [ 0.0, -0.6, -20.0 ]
      orientation: [ 0.0, 0.0, 0.0 ]
      scale: [ 4.0, 4.0, 4.0 ]
    ScriptableComponent:
      script: player.lua
    CameraComponent:
      camera:
        fov: 45.0
        aspect: 1.7777777779
        nearPlane: 1.0
        farPlane: 500.0
    RenderableComponent:
      active: true
      model: enemy.obj
      shader: simple.shader
    PlayerControllerComponent:
      radius: 0.5
      height: 1.8
      speed: 10.50
      moveDrag: 7.0
      fallDrag: 5.0

